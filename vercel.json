{
    "version": 2,
    "builds": [
        {
            "src": "manage.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/manage.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "tributepage.settings"
    }
}